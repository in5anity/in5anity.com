{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "packageManager": "yarn"
  },
  "projects": {
    "in5anity": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "packages/website",
      "sourceRoot": "packages/website/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "packages/website/dist/in5anity/browser",
            "index": "packages/website/src/index.html",
            "main": "packages/website/src/main.ts",
            "polyfills": "packages/website/src/polyfills.ts",
            "tsConfig": "packages/website/tsconfig.app.json",
            "aot": true,
            "assets": [
              "packages/website/src/favicon.ico",
              "packages/website/src/assets"
            ],
            "styles": [
              "packages/website/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "packages/website/src/environments/environment.ts",
                  "with": "packages/website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "in5anity:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "in5anity:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "in5anity:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "packages/website/src/test.ts",
            "polyfills": "packages/website/src/polyfills.ts",
            "tsConfig": "packages/website/tsconfig.spec.json",
            "karmaConfig": "packages/website/karma.conf.js",
            "assets": [
              "packages/website/src/favicon.ico",
              "packages/website/src/assets"
            ],
            "styles": [
              "packages/website/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "packages/website/tsconfig.app.json",
              "packages/website/tsconfig.spec.json",
              "packages/website/e2e/tsconfig.json",
              "packages/website/tsconfig.server.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "packages/website/e2e/protractor.conf.js",
            "devServerTarget": "in5anity:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "in5anity:serve:production"
            }
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "packages/website/dist/in5anity/server",
            "main": "packages/website/server.ts",
            "tsConfig": "packages/website/tsconfig.server.json"
          },
          "configurations": {
            "production": {
              "outputHashing": "media",
              "fileReplacements": [
                {
                  "replace": "packages/website/src/environments/environment.ts",
                  "with": "packages/website/src/environments/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": true
            }
          }
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "options": {
            "browserTarget": "in5anity:build",
            "serverTarget": "in5anity:server"
          },
          "configurations": {
            "production": {
              "browserTarget": "in5anity:build:production",
              "serverTarget": "in5anity:server:production"
            }
          }
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "browserTarget": "in5anity:build:production",
            "serverTarget": "in5anity:server:production",
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {}
          }
        }
      }
    }
  },
  "defaultProject": "in5anity"
}
